{
  "manifest_version": 3,
  "name": "Multi-Search Extension",
  "version": "1.0",
  "description": "Search multiple websites simultaneously.",
  "permissions": [
    "tabs",
    "storage",
    "activeTab",
    "scripting",
    "windows",

    "storage",
    "notifications"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Command+Q"
      }
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}


